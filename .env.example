APP_NAME="Laravel Dashboard"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=https://laravel-dashboard.local

DEBUGBAR_ENABLED=false

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# use for Heroku
# DATABASE_URL=postgres://user:password@ecIP-ADDR.REGION.compute.amazonaws.com:5432/db
# DB_CONNECTION=pgsql

DB_CONNECTION=mysql
DB_HOST=laravel_db
DB_PORT=3306
DB_DATABASE=laravel-dashboard
DB_USERNAME=root
DB_PASSWORD=root

DB_ROOT_PASSWORD=root
DB_MYSQL_ATTR_SSL_KEY=.docker/certs/localhost.crt

# touch database/database.sqlite
# DB_CONNECTION=sqlite
# DB_DATABASE=database/database.sqlite

BROADCAST_DRIVER=pusher
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=rabbitmq
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=laravel_redis
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_PREFIX=null

RABBITMQ_HOST=laravel_rabbitmq
RABBITMQ_VHOST=/
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_QUEUE=jobs

MAIL_MAILER=smtp
MAIL_HOST=laravel_mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="www-admin@laravel-dashboard.local"
MAIL_FROM_NAME="${APP_NAME}"

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=eu

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

SSL_CRT_PATH=.docker/certs/localhost.crt
SSL_KEY_PATH=.docker/certs/localhost.key

L5_SWAGGER_GENERATE_ALWAYS=true
API_TOKEN=

HEROKU=false
